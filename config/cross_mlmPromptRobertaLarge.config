test_dataset_type = agnews_s1,cs_wiki_s1,scierc_s1,SST-2_s1,IMDB_s1,agnews_s2,cs_wiki_s2,scierc_s2,SST-2_s2,IMDB_s2
test_formatter_type = cross_mlmPrompt
