[train] #train parameters
epoch = 100
batch_size = 16

reader_num = 0

optimizer = adamw
learning_rate = 0.001
weight_decay = 0
step_size = 1
lr_multiplier = 1

max_len = 128


[distributed]
use = True
backend = nccl

[eval] #eval parameters
